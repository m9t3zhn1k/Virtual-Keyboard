{
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "env": {
      "es6": true,
      "browser": true,
      "node": true
  },
  "rules": {
    "import/extensions": ["error", "always"],
    "linebreak-style": ["error", "windows"],
    "import/prefer-default-export": "off"
  },
  "extends": ["airbnb-base"]
}